{
  "name": "webpack-boilerplate",
  "version": "1.0.2",
  "description": "Webpack 4 - SCSS - PUG boilerplate",
  "main": "index.js",
  "loglevel": "silent",
  "scripts": {
    "build": "npm run clean -s && npm run production -s",
    "clean": "rimraf dist",
    "dev": "cross-env webpack --config ./webpack/webpack.dev.js",
    "production": "cross-env webpack --progress --hide-modules --config ./webpack/webpack.prod.js",
    "start": "webpack-dev-server --open --config ./webpack/webpack.dev.js"
  },
  "keywords": [
    "webpack 4",
    "boilerplate",
    "scss",
    "pug",
    "ES6",
    "performance",
    "babel"
  ],
  "author": "Varun N R",
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead",
    "ie >= 11"
  ],
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "babel-eslint": "10.0.2",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "5.0.4",
    "core-js": "^3.6.5",
    "cross-env": "6.0.3",
    "css-hot-loader": "1.4.4",
    "css-loader": "3.2.0",
    "eslint": "6.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-compat": "3.2.0",
    "extract-loader": "3.1.0",
    "file-loader": "4.2.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-loader": "0.5.5",
    "html-webpack-harddisk-plugin": "1.0.1",
    "html-webpack-plugin": "3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "ip": "1.1.5",
    "mini-css-extract-plugin": "0.8.0",
    "node": "^14.2.0",
    "node-sass-json-importer": "4.1.0",
    "npm-install-webpack-plugin": "^4.0.5",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "path": "0.12.7",
    "postcss-loader": "3.0.0",
    "pug": "2.0.4",
    "pug-html-loader": "1.1.5",
    "pug-loader": "2.4.0",
    "raw-loader": "3.1.0",
    "regenerator-runtime": "^0.13.5",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "source-map-loader": "0.2.4",
    "style-loader": "1.0.0",
    "stylelint": "11.1.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-order": "3.1.1",
    "stylelint-scss": "3.12.0",
    "terser-webpack-plugin": "^3.0.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "3.3.9",
    "webpack-dev-server": "^3.11.0",
    "webpack-format-messages": "2.0.5",
    "webpack-merge": "4.2.2",
    "webpack-messages": "2.0.4"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "node-sass": "^4.14.1",
    "sass-mq": "5.0.1",
    "whatwg-fetch": "3.0.0"
  }
}
